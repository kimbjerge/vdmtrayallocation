class Discharge
	instance variables 
		private number : nat := 0;
	  	private parcels : seq of Item := [];

	types 

	values 

	operations 

    public Discharge: nat ==> Discharge
    Discharge(n) ==
    (
    	number := n;
    );
    
	public ParcelInDischarge: Item ==> ()
	ParcelInDischarge (p) ==
	(
		parcels := parcels ^ [p];
	);
		
	functions 

	traces 

end Discharge